<div class="container-fluid">
  <div class="navbar">
    <div class="navbar-inner">
      <a class="brand" href="#">EmberReader</a>
      <ul class="nav pull-right">
        <li>{{#linkTo 'feeds'}}Feeds{{/linkTo}}</li>
        {{#if App.Session.authToken}}
          <li>{{#linkTo 'session.destroy'}}Logout{{/linkTo}}</li>
        {{else}}
          <li>{{#linkTo 'session.new'}}Login{{/linkTo}}</li>
        {{/if}}
      </ul>
    </div>
  </div>
  <div class="main-content">
    {{outlet}}
  </div>
</div>
