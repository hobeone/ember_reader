<div class="row-fluid">
<div class="span3">
  <ul class="unstyled" id="feed-list">
    {{#if model.isLoaded}}
      {{#each i in model}}
      <li><i class="icon-list"></i>{{#linkTo "feed" i}}{{i.title}} ({{i.unread}}){{/linkTo}}</li>
      {{/each}}
    {{else}}
    <p class="loading">Loading Feeds...</p>
    {{/if}}
</ul>
</div>
<div class="span9">
  {{outlet}}
</div>
</div>

